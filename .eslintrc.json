{
    "extends": "standard",
    "rules": {
      "quotes": 0,
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "no-restricted-syntax": 0,
      "consistent-return": 0,
      "prefer-const": 0,
      "no-path-concat": 0,
      "prefer-template": 0,
      "no-undef": 0,
      "object-shorthand": 0,
      "comma-dangle": 0,
      "semi": 0,
      "space-before-function-paren": 0,
      "no-unused-vars": 0
    }
}
